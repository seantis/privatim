<metal:block use-macro="main_template"
             xmlns="http://www.w3.org/1999/xhtml"
             xmlns:metal="http://xml.zope.org/namespaces/metal"
         i18n:domain="privatim">

    <div metal:fill-slot="content">


        <div metal:fill-slot="content">

            <div class="row mt-4 mb-4">
                <div class="col-md-6">
                    <h1><tal:block i18n:translate>Working Group</tal:block> ${group.name}</h1>
                </div>
                <div class="col-md-6 text-end">
                        <a i18n:translate href="${request.route_url('add_meeting')}" class="btn btn-primary" role="button" tabindex="1" i18n:translate>Add Meeting</a>
                </div>
            </div>

        </div>

    </div>
</metal:block>
